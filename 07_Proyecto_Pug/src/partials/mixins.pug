-let aSecciones = [{label: 'Inicio', url: "./"}, {label: 'Porfolio', url: "./port.html"}, {label: 'Clientes', url: "./cli.html"}, {label: 'Acerca de', url: "./about.html"}]
-let aSocial = ['Facebook', 'Twitter', 'Github']


mixin menuSocial(aItems)
    nav 
        ul 
            each item in aItems 
                li: a(href="#"+item.toLowerCase())= item

mixin menu(aItems)
    nav
        ul
            each item in aItems
                    li: a(href=item.url)= item.label

